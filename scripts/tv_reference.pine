//@version=5
indicator("Barb Ref", overlay=false)
plot(ta.rsi(close, 14), "RSI")
plot(ta.atr(14), "ATR")
plot(ta.cci(hlc3, 20), "CCI")
plot(ta.stoch(close, high, low, 14), "StochK")
plot(ta.wpr(14), "WilliamsR")
plot(ta.mfi(hlc3, 14), "MFI")
plot(ta.tr, "TR")
plot(ta.atr(14) / close * 100, "NATR")
[bbMid, bbUp, bbLow] = ta.bb(close, 20, 2)
plot(bbUp, "BB_Upper")
plot(bbMid, "BB_Middle")
plot(bbLow, "BB_Lower")
plot(ta.bbw(close, 20, 2), "BB_Width")
plot((close - bbLow) / (bbUp - bbLow), "BB_PctB")
[kcMid, kcUp, kcLow] = ta.kc(close, 20, 1.5, true)
plot(kcUp, "KC_Upper")
plot(kcMid, "KC_Middle")
plot(kcLow, "KC_Lower")
plot((kcUp - kcLow) / kcMid, "KC_Width")
[macdLine, signalLine, histLine] = ta.macd(close, 12, 26, 9)
plot(macdLine, "MACD")
plot(signalLine, "MACD_Signal")
plot(histLine, "MACD_Hist")
[diPlus, diMinus, adxVal] = ta.dmi(14, 14)
plot(diPlus, "Plus_DI")
plot(diMinus, "Minus_DI")
plot(adxVal, "ADX")
[stVal, stDir] = ta.supertrend(3, 10)
plot(stVal, "SuperTrend")
plot(stDir, "SuperTrend_Dir")
plot(ta.sar(0.02, 0.02, 0.2), "SAR")
